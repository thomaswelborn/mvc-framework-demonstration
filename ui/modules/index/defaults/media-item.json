{
  "models": {},
  "views": {
    "view": {
      "attributes": {
        "id": "index-media-item",
        "class": "media-item"
      }
    }
  },
  "controllers": {
    "navigation": {
      "models": {},
      "views": {
        "view": {
          "attributes": {
            "id": "index-media-item-navigation"
          }
        }
      },
      "controllers": {
        "buttons": [
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": true
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "id": "index-media-item-navigation-info-button",
                  "class": "button icon-info",
                  "data-action": "info"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false 
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "id": "index-media-item-navigation-favorite-button",
                  "class": "button icon-star-full",
                  "data-action": "favorite"
                }
              }
            }
          }
        ]
      }
    }
  }
}
{
  "models": {
    "ui": {
      "defaults": {
        "toggled": false
      }
    }
  },
  "views": {
    "view": {
      "attributes": {
        "id": "application-toggle-navigation",
        "class": "navigation toggle-navigation",
        "data-toggled": false
      }
    }
  },
  "controllers": {
    "toggle": {
      "models": {
        "ui": {
          "defaults": {}
        }
      },
      "views": {
        "view": {
          "attributes": {
            "id": "application-toggle-navigation-toggle-button",
            "class": "button toggle-button icon-menu"
          }
        }
      }
    },
    "subnavigation": {
      "models": {},
      "views": {
        "view": {
          "attributes": {
            "id": "application-toggle-navigation-subnavigation",
            "class": "navigation subnavigation"
          }
        }
      },
      "controllers": {
        "buttons": [
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false,
                  "textContent": "PROFILE"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-profile",
                  "data-href": "/account/profile",
                  "data-target": "_self"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false,
                  "textContent": "MY PHOTOS"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-images",
                  "data-href": "/photos",
                  "data-target": "_self"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false,
                  "textContent": "MY FAVORITES"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-star-empty",
                  "data-href": "/favorites",
                  "data-target": "_self"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false,
                  "textContent": "UPLOAD"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-box-remove",
                  "data-href": "/upload",
                  "data-target": "_self"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": false,
                  "noAuth": true,
                  "textContent": "LOGIN"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-enter",
                  "data-href": "/account/login",
                  "data-target": "_self"
                }
              }
            }
          },
          {
            "models": {
              "ui": {
                "defaults": {
                  "auth": true,
                  "noAuth": false,
                  "textContent": "LOGOUT"
                }
              }
            },
            "views": {
              "view": {
                "attributes": {
                  "class": "button subnavigation-button icon-exit",
                  "data-href": "account/logout",
                  "data-target": "_self"
                }
              }
            }
          }
        ]
      }
    }
  }
}
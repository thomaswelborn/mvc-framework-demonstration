{
  "models": {
    "ui": {
      "localStorage": {
        "sync": true,
        "endpoint": "/api/photos/select-navigation/settings"
      },
      "defaults": {
        "selectedOption": "ASC"
      }
    }
  },
  "views": {
    "view": {
      "attributes": {
        "id": "photos-select-navigation"
      },
      "children": [
        {
          "tag": {
            "name": "select",
            "close": true
          },
          "attributes": {
            "class": "select"
          },
          "children": [
            {
              "tag": {
                "name": "option",
                "close": true
              },
              "attributes": {
                "id": "photos-select-navigation-option-ascending",
                "value": "ASC"
              },
              "innerText": "Oldest"
              },
            {
              "tag": {
                "name": "option",
                "close": true
              },
              "attributes": {
                "id": "photos-select-navigation-option-descending",
                "value": "DESC"
              },
              "innerText": "Newest"
            }
          ]
        },
        {
          "tag": {
            "name": "nav",
            "close": true
          },
          "attributes": {
            "class": "subnavigation"
          }
        }
      ]
    }
  },
  "controllers": {}
}
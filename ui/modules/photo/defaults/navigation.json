{
  "models": {
    "ui": {}
  },
  "views": {
    "view": {
      "attributes": {
        "id": "photo-navigation",
        "class": "navigation"
      }
    }
  },
  "controllers": {
    "buttons": [
      {
        "models": {
          "ui": {
            "defaults": {
              "auth": true,
              "noAuth": false,
              "textContent": "DELETE"
            }
          }
        },
        "views": {
          "view": {
            "attributes": {
              "id": "photo-navigation-delete",
              "class": "button",
              "data-action": "delete"
            }
          }
        }
      },
      {
        "models": {
          "ui": {
            "defaults": {
              "auth": true,
              "noAuth": false,
              "textContent": "CLOSE"
            }
          }
        },
        "views": {
          "view": {
            "attributes": {
              "id": "photo-navigation-close",
              "class": "button",
              "data-action": "close"
            }
          }
        }
      }
    ]
  }
}
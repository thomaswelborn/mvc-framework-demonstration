{
  "name": "mvc-framework-demonstration",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run documents\" \"npm run scripts\" \"npm run styles\" \"npm run server\" \"npm run copy\"",
    "documents": "nodemon -e json,ejs -w ./documents -w ./processes/ejs.js ./processes/ejs.js",
    "scripts": "rollup -c ./processes/rollup.config.js -w",
    "styles": "nodemon  -e scss -w ./ui ./processes/node-sass.js",
    "copy": "nodemon -e jpg,gif,svg,png -w ./media -w ./processes/copy.js ./processes/copy.js",
    "server": "browser-sync start --config ./processes/bs-config.js",
    "generate": "node ./generator",
    "generate:files": "node ./generator -files"
  },
  "description": "Allows data object configuration of Gulp Libraries to become Gulp Tasks, Series, and Parallels.  \r Instead of authoring new Gulpfiles for each project, use Gulp Boilerplate to automatically configure Gulp Processes based on JSON objects.",
  "author": "Thomas Welborn",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "browser-sync": "^2.26.7",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "decache": "^4.5.1",
    "ejs": "^3.1.5",
    "globby": "^11.0.1",
    "js-beautify": "^1.10.0",
    "mvc-framework": "git+https://gitlab.com/thomas.welborn/mvc-framework.git",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-ejs": "^2.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "yargs": "^13.3.2"
  },
  "dependencies": {}
}
